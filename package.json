{
  "name": "dala-org-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animejs": "^2.2.0",
    "bulma": "^0.6.2",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.5.3",
    "react-headroom": "^2.2.2",
    "react-popupbox": "^2.0.8",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.7.6",
    "react-slick": "^0.18.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "aws s3 sync build/ s3://dalaorg-website-bucket --profile dalaPrimary && aws cloudfront create-invalidation --distribution-id E22LF1XX2HNXBP --paths '/*' --profile dalaPrimary"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
